       defineSizeMapping(mapping_mobile_top).
       setCollapseEmptyDiv(true,true).
       addService(googletag.pubads());
    gptAdSlots['pfr'.concat('_','mobile_top')].set("adsense_channel_ids", "3900428057+9622926853");
 
    gptAdSlots['pfr'.concat('_','mobile_1')] =  googletag.defineSlot('/117033896/ak_mobile_1', [[300,250],[320,100],[234,60]], 'akunit_mobile_1').
