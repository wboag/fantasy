       addSize([0, 0], []).
       build();

    mapping_desktop_1 = googletag.sizeMapping().
       addSize([970, 700], [[970, 90],[728,90],[600,340]]).
       addSize([728, 500], [728,90],[600,340]).
       addSize([0, 0], []).
       build();

    mapping_info300 = googletag.sizeMapping().
       addSize([736, 700], [300, 250]).
       addSize([0, 0], []).
       build();

    mapping_mobile_1 = googletag.sizeMapping().
       addSize([728, 500], []).
       addSize([320, 100], [[300, 250],[320, 100],[234,60]]).
       addSize([300, 100], [[300, 250],[234,60]]).
       addSize([234, 100], [234,60]).
       addSize([0, 0], []).
       build();

    mapping_mobile_top = googletag.sizeMapping().
       addSize([728, 500], []).
       addSize([728, 500], [728,90]).
       addSize([468, 300], [468,60]).
       addSize([320, 100], [[320, 50],[320, 100],[234,60]]).
       addSize([234, 60], [234,60]).
       addSize([0, 0], []).
       build();
 
    mapping_other_1 = googletag.sizeMapping().
       addSize([970, 700], [[970, 90],[728,90]]).
       addSize([728, 500], [728,90]).
       addSize([300, 10], [300, 250]).
       addSize([0, 0], []).
       build();

    mapping_srcom728 = googletag.sizeMapping().
       addSize([800, 500], [728, 90]).
       addSize([0, 0], []).
       build();
    gptAdSlots['pfr'.concat('_','info300')] =  googletag.defineSlot('/117033896/ak_info300', [300,250], 'akunit_info300').
       defineSizeMapping(mapping_info300).
       addService(googletag.pubads());
    gptAdSlots['pfr'.concat('_','info300')].set("adsense_channel_ids", "3900428057+5192727250");
 
    gptAdSlots['pfr'.concat('_','srcom728')] =  googletag.defineSlot('/117033896/ak_srcom728', [728,90], 'akunit_srcom728').
       defineSizeMapping(mapping_srcom728).
       addService(googletag.pubads());
    gptAdSlots['pfr'.concat('_','srcom728')].set("adsense_channel_ids", "3900428057+3715994054");
 
